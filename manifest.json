{
  "name": "dislikeez",
  "version": "1.0.0",
  "description": "ilyuha dislikeez",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/ilyusha.png",
      "32": "images/ilyusha32.png",
      "48": "images/ilyusha48.png",
      "128": "images/ilyusha128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/watch*"],
      "js": ["dislikeez.js"]
    }
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
